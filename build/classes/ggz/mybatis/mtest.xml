<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mTest"> <!-- namespace : 일종의 식별역할의 변수 -->
	<select id="selectMIdx" resultType="Mtest" parameterType="int">
		SELECT * FROM MTest WHERE idx=#{idx}
	</select>
	<select id="cntMIdx" resultType="int">
		SELECT COUNT(*) FROM MTest
	</select>
</mapper>








